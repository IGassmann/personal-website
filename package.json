{
  "name": "@igassmann/personal-website",
  "description": "Igor Gassmann's personal website",
  "keywords": [
    "react",
    "nextjs",
    "vercel"
  ],
  "license": "Unlicense",
  "homepage": "https://igassmann.me/",
  "repository": "https://github.com/IGassmann/personal-website",
  "bugs": "https://github.com/IGassmann/personal-website/issues",
  "author": "Igor Gassmann <igor@igassmann.me> (https://igassmann.me)",
  "main": "n/a",
  "scripts": {
    "dev": "next dev",
    "lint": "next lint --dir ./src/ --dir ./scripts/ --ignore-path ./.gitignore --fix",
    "format": "prettier . --ignore-path ./.gitignore --write",
    "build:sitemap": "node --loader ts-node/esm scripts/generateSitemap.ts",
    "build:feeds": "node --loader ts-node/esm scripts/generateFeeds.ts",
    "prebuild": "pnpm build:sitemap & pnpm build:feeds",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/react-dom": "18.2.22",
    "dayjs": "1.11.5",
    "gray-matter": "4.0.3",
    "lodash": "4.17.21",
    "next": "14.1.3",
    "next-seo": "4.28.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "7.1.1",
    "react-syntax-highlighter": "15.5.0",
    "remark-directive": "2.0.1",
    "remark-directive-rehype": "0.4.1",
    "remark-parse": "11.0.0",
    "remark-unwrap-images": "3.0.1",
    "sass": "1.51.0",
    "tailwindcss": "3.0.23"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/react": "18.2.67",
    "@types/react-syntax-highlighter": "13.5.2",
    "@types/tailwindcss": "2.2.4",
    "autoprefixer": "10.4.0",
    "eslint": "8.24.0",
    "eslint-config-next": "14.1.3",
    "feed": "4.2.2",
    "globby": "13.1.1",
    "husky": "9.0.11",
    "lint-staged": "12.3.7",
    "postcss": "8.4.16",
    "prettier": "2.5.1",
    "rehype-format": "3.1.0",
    "rehype-stringify": "8.0.0",
    "rehype-truncate": "1.2.2",
    "rehype-urls": "1.1.1",
    "remark-rehype": "8.1.0",
    "sitemap": "7.1.1",
    "ts-node": "10.9.1",
    "tsc-files": "1.1.3",
    "typescript": "5.4.2",
    "unified": "10.1.2"
  },
  "engines": {
    "node": "18.x",
    "pnpm": "8.15.5"
  },
  "packageManager": "pnpm@8.15.5",
  "private": true
}
